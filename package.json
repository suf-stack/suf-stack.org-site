{
  "name": "suf-stack.org-site",
  "version": "1.0.0",
  "description": "My kryptonian Nuxt.js project",
  "author": "Brad Ito",
  "private": true,
  "scripts": {
    "dev": "cross-env NODE_ENV=development nodemon server/index.js --watch server",
    "build": "nuxt build",
    "start": "cross-env NODE_ENV=production node server/index.js",
    "generate": "rm -rf dist public && nuxt generate",
    "deploy": "npm run generate && mv dist public"
  },
  "dependencies": {
    "@nuxtjs/gtm": "^2.4.0",
    "buefy": "^0.9.23",
    "bulma": "^0.9.4",
    "cross-env": "^7.0.3",
    "koa": "^2.14.2",
    "nuxt": "^2.17.1",
    "nuxt-buefy": "^0.4.25",
    "vue-code-highlight": "^0.7.8",
    "vue-scrollto": "^2.20.0"
  },
  "devDependencies": {
    "@nuxtjs/style-resources": "^1.2.1",
    "node-sass": "^8.0.0",
    "nodemon": "^3.0.1",
    "sass-loader": "^10.4.1"
  }
}
